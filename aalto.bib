  shorttitle = {Avatars {{Versus Agents}}},
  author = {Fox, Jesse and Ahn, Sun Joo (Grace) and Janssen, Joris H. and Yeykelis, Leo and Segovia, Kathryn Y. and Bailenson, Jeremy N.},
  year = {2015},
