@article{duttaMicrofluidicDevicesLabelFree2018,
  title = {Microfluidic {{Devices}} for {{Label}}-{{Free DNA Detection}}},
  author = {Dutta, Gorachand and Rainbow, Joshua and Zupancic, Uros and Papamatthaiou, Sotirios and Estrela, Pedro and Moschou, Despina},
