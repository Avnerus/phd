@article{shintakeSoftPneumaticGelatin2017,
  title = {Soft {{Pneumatic Gelatin Actuator}} for {{Edible Robotics}}},
  author = {Shintake, Jun and Sonar, Harshal and Piskarev, Egor and Paik, Jamie and Floreano, Dario},
